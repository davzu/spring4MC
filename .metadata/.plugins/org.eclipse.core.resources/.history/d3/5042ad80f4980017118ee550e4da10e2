package com.mitocode;

import com.mitocode.dao.IPersonaDAO;
import com.mitocode.dao.PersonaDAOImpl;
import com.mitocode.model.Persona;
import com.mitocode.service.IPersonaService;
import com.mitocode.service.PersonaServiceImpl;

public class App {
	
	public static void main(String[] args){
		IPersonaDAO dao = new PersonaDAOImpl();
		IPersonaService service = new PersonaServiceImpl(dao);
		service.create(new Persona(1,"Mito","Code"));
	}

}
